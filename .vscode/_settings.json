  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": ["comment"],
        "settings": {
          "foreground": "#6a7a67",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["string"],
        "settings": {
          "foreground": "#8fa889"
        }
      },
      {
        "scope": ["keyword"],
        "settings": {
          "foreground": "#7a9a73",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["variable", "parameter"],
        "settings": {
          "foreground": "#c8d6c3"
        }
      },
      {
        "scope": ["function", "method"],
        "settings": {
          "foreground": "#a8c5a0"
        }
      },
      {
        "scope": ["number", "constant"],
        "settings": {
          "foreground": "#9aaf94"
        }
      },
      {
        "scope": ["type", "class"],
        "settings": {
          "foreground": "#7a9a73"
        }
      },
      {
        "scope": ["property"],
        "settings": {
          "foreground": "#a8c5a0"
        }
      }
    ]
  }
}

